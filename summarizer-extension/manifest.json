{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0",
  "action": {
    
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["windows", "activeTab", "scripting", "tabs"],
 "content_scripts": [
  {
    "matches": ["https://*/*", "http://*/*"], 
    "js": ["js/contentScript.js"]
  }
],
"web_accessible_resources": [
  {
    "resources": ["js/index.html"],
    "matches": ["<all_urls>"]
  }
]
}

